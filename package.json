{
  "name": "@movesfinancial/mongoose-transact-utils",
  "version": "0.1.3",
  "description": "Helper methods for Mongoose and MongoDB transactions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn lint && yarn compile",
    "lint": "eslint 'src/**/*.{js,ts}' --ignore-pattern 'src/**/__test__/*' --fix",
    "test": "jest",
    "build:fast": "yarn compile",
    "compile": "rimraf lib && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepack": "yarn build",
    "postversion": "git push && git push --tags"
  },
  "repository": "git@github.com:movesfinancial/mongoose-transact-utils.git",
  "keywords": [
    "mongooose",
    "mongodb",
    "transactions"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "@movesfinancial/typescript-log": "^2.1.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "29.6.2",
    "jest-config": "29.6.2",
    "mongodb-memory-server": "^8.13.0",
    "mongoose": "^7.4.2",
    "prettier": "^3.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "9.1.1",
    "typedoc": "0.24.8"
  },
  "peerDependencies": {
    "mongoose": "7.4.1"
  },
  "author": "CashPositive",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cashpositive/mongoose-transact-utils/issues"
  },
  "homepage": "https://github.com/cashpositive/mongoose-transact-utils#readme"
}
